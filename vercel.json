{
  "version": 2,
  "builds": [
    {
      "src": "project/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "project/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
} 
